{
  "manifest_version": 3,
  "name": "Capstone Browser Extension",
  "version": "1.0",
  "description": "Browser extension for our capstone course.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon16.png",
      "32": "favicon32.png"
    }
  },
  "icons": {
    "16": "favicon16.png",
    "32": "favicon32.png",
    "48": "favicon48.png",
    "128": "favicon128.png"
  }
}

